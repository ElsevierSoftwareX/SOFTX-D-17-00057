Cell[BoxData[{
 RowBox[{
  RowBox[{"BeginPackage", "[", "\"\<ProcessWithExpACF`\>\"", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"RandomVariateExpACF", "::", "usage"}], "=", 
    "\"\<RandomVariateExpACF[\!\(\*
StyleBox[\"dist\",
FontSlant->\"Italic\"]\)\!\(\*
StyleBox[\",\",
FontSlant->\"Italic\"]\)\!\(\*
StyleBox[\"n\",
FontSlant->\"Italic\"]\)\!\(\*
StyleBox[\",\",
FontSlant->\"Italic\"]\)\!\(\*
StyleBox[\"\[CapitalDelta]t\",
FontSlant->\"Italic\"]\),\!\(\*
StyleBox[\"\[Lambda]\",
FontSlant->\"Italic\"]\),\!\(\*
StyleBox[\"x0\",
FontSlant->\"Italic\"]\)] gives a list of \!\(\*
StyleBox[\"n\",
FontSlant->\"Italic\"]\) pseudorandom variate process from the symbolic \
distribution \!\(\*
StyleBox[\"dist\",
FontSlant->\"Italic\"]\), with exponential autocorrelation function with \
correlation time \!\(\*
StyleBox[\"\[Lambda]\",
FontSlant->\"Italic\"]\), sample time \!\(\*
StyleBox[\"\[CapitalDelta]t\",
FontSlant->\"Italic\"]\) and initial value \!\(\*
StyleBox[\"x0\",
FontSlant->\"Italic\"]\)\!\(\*
StyleBox[\".\",
FontSlant->\"Italic\"]\)\>\""}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"RandomVariateExpACF", "[", 
   RowBox[{
    StyleBox["dist_",
     FontSlant->"Italic"], 
    StyleBox[",",
     FontSlant->"Italic"], 
    StyleBox["n_",
     FontSlant->"Italic"], 
    StyleBox[",",
     FontSlant->"Italic"], "\[CapitalDelta]t_", ",", "\[Lambda]_", ",", 
    StyleBox["x0_",
     FontSlant->"Italic"]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "vals", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"$Assumptions", "=", 
      RowBox[{
       RowBox[{"x", "\[Element]", "Reals"}], "&&", 
       RowBox[{"\[Lambda]", ">", "0"}], "&&", 
       RowBox[{"\[Lambda]", "\[Element]", "Reals"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p", "=", 
      RowBox[{"PDF", "[", 
       RowBox[{"dist", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"m", "=", 
      RowBox[{"Mean", "[", "dist", "]"}]}], ";", 
     RowBox[{"K2", "=", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{"-", "2"}], 
        FractionBox["\[Lambda]", 
         RowBox[{"p", " "}]], 
        RowBox[{"Integrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"x", "-", "m"}], ")"}], "p"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"-", "Infinity"}], ",", "x"}], "}"}]}], "]"}]}], "]"}]}], 
     ";", 
     RowBox[{"(*", 
      RowBox[{
       SuperscriptBox["g", "2"], "[", "x", "]"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"g", "[", "x_", "]"}], "=", 
      RowBox[{"Sqrt", "[", "K2", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"gg", "[", "x_", "]"}], "=", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{"Sqrt", "[", "K2", "]"}], " ", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"Sqrt", "[", "K2", "]"}], ",", "x"}], "]"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"rnd", "=", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"NormalDistribution", "[", "]"}], ",", 
        RowBox[{"n", "-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"y_", ",", "\[Xi]_"}], "]"}], ":=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"1", "+", 
         RowBox[{"\[Lambda]", " ", "\[CapitalDelta]t"}]}]], 
       RowBox[{"(", 
        RowBox[{"y", "+", " ", 
         RowBox[{"\[Lambda]", " ", "m", " ", "\[CapitalDelta]t"}], "+", 
         RowBox[{
          RowBox[{"g", "[", "y", "]"}], 
          SqrtBox["\[CapitalDelta]t"], "\[Xi]"}], "+", 
         RowBox[{
          FractionBox["1", "2"], 
          RowBox[{"gg", "[", "y", "]"}], "\[CapitalDelta]t", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["\[Xi]", "2"], "-", "1"}], ")"}]}]}], ")"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"vals", "=", " ", 
      RowBox[{"FoldList", "[", 
       RowBox[{"f", ",", "x0", ",", "rnd"}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
